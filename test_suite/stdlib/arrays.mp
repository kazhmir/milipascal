	Num:i8;
end

struct I16A begin
	Num:i16;
end

struct I32A begin
	Num:i32;
end

struct I64A begin
	Num:i64;
end

struct U8A begin
	Num:u8;
end

struct U16A begin
	Num:u16;
end

struct U32A begin
	Num:u32;
end

struct U64A begin
	Num:u64;
end

struct PtrA begin
	Ptr:ptr;
end

struct BoolA begin
	Bool:bool;
end
